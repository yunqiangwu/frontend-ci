{
  "name": "frontend-ci",
  "description": "auto pull and robot of weixin qq and  auto restart deploy server",
  "version": "0.5.3",
  "main": "lib/index.js",
  "author": "yunqiangwu",
  "repository": "yunqiangwu/frontend-ci",
  "license": "MIT",
  "scripts": {
    "compile": "babel src --out-dir lib && rm -rf ./lib/public && cp -r ./src/public ./lib/public && cd boilerplate && ../node_modules/.bin/roadhog build",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "start": "cd boilerplate && cross-env PORT=9898 ../node_modules/.bin/roadhog server",
    "prepublish": "yarn run compile",
    "test": "babel-node ./node_modules/.bin/isparta cover _mocha"
  },
  "files": [
    "bin",
    "lib",
    "boilerplate"
  ],
  "devDependencies": {
    "babel-cli": "*",
    "babel-plugin-dva-hmr": "^0.4.1",
    "babel-plugin-import": "^1.6.7",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "6.x",
    "babel-preset-es2016": "6.x",
    "babel-preset-es2017": "6.x",
    "chai": "*",
    "chalk": "^2.1.0",
    "chokidar": "^1.7.0",
    "commander": "^2.11.0",
    "coveralls": "*",
    "cross-env": "^5.1.4",
    "fs-extra": "^4.0.1",
    "is-windows": "^1.0.1",
    "isparta": "*",
    "mocha": "*",
    "portfinder": "^1.0.13",
    "roadhog": "^2.3.0",
    "shelljs": "^0.7.8",
    "sinon": "*"
  },
  "dependencies": {
    "antd": "^3.3.1",
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-prettier": "^2.6.0",
    "http-proxy": "^1.16.2",
    "is-windows": "^1.0.1",
    "killport2": "^1.0.4",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-proxy": "^0.9.0",
    "koa-route": "^3.2.0",
    "koa-static": "^4.0.2",
    "koa-websocket": "^4.1.0",
    "node-static": "^0.7.10",
    "optimist": "^0.6.1",
    "os": "^0.1.1",
    "qrcode-terminal": "^0.11.0",
    "shelljs": "^0.7.8",
    "tree-kill": "^1.2.0",
    "ws": "^3.3.2"
  },
  "bin": {
    "frontend-ci": "./bin/frontend-ci"
  }
}
